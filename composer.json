{
    "name": "oanhnn/laravel-presets",
    "type": "library",
    "description": "Laravel Preset Command polyfill for Laravel 5.4",
    "keyworks": ["laravel","package","frontend","vue","react"],
    "license": "MIT",
    "homepage": "https://github.com/oanhnn/laravel-presets",
    "support": {
        "issues": "https://github.com/oanhnn/laravel-presets/issues",
        "source": "https://github.com/oanhnn/laravel-presets"
    },
    "authors": [
        {
            "name": "Oanh Nguyen",
            "email": "oanhnn.bk@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravel\\Presets\\": "src/"
        }
    },
    "require": {
        "php": ">=7.0",
        "illuminate/contracts": "5.4.*",
        "illuminate/support": "5.4.*",
        "illuminate/filesystem": "5.4.*",
        "illuminate/console": "5.4.*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.5",
        "phpunit/phpunit": "~5.7",
        "mockery/mockery": "~0.9"
    },
    "config": {
        "preferred-install": "dist"
    }
}
